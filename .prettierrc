{
  "parser": "typescript",
  "printWidth": 140,
  "tabWidth": 4,
  "useTabs": false,
  "semi": false,
  "arrowParens": "avoid",
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "importOrder": ["^@[az]*/(.*)$", "^[.]"],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "parser": "json",
        "tabWidth": 2,
        "trailingComma": "none",
        "singleQuote": false
      }
    }
  ]
}
