{
  "name": "ag-grid-column-builder",
  "version": "1.0.3",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ilikejames/ag-grid-column-builder/"
  },
  "description": "A type safe column builder for ag-grid that automatically correctly types the definitions handler arguments",
  "main": "dist/index.cjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "license": "MIT",
  "peerDependencies": {
    "@ag-grid-community/core": ">=20.0.0 <30.0.0"
  },
  "devDependencies": {
    "@ag-grid-community/core": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.36.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "yarn tsc --noEmit",
    "prepublish": "yarn build && yarn test"
  }
}
